{
  "name": "discoursio-web",
  "version": "0.1.0",
  "private": "true",
  "license": "MIT",
  "description": "Collaborative publishing",
  "repository": {
    "url": "https://github.com/Discours/discoursio-web"
  },
  "scripts": {
    "build": "svelte-kit build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "codegen": "graphql-codegen",
    "debug": "NODE_OPTIONS=\"--inspect-brk\" svelte-kit dev",
    "dev": "svelte-kit dev",
    "fix": "eslint --fix --ignore-path .gitignore src",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. src",
    "format:eslint": "npm run lint:eslint -- --fix",
    "format:prettier": "npm run lint:prettier -- --write",
    "gen": "node precompiler.js",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore src",
    "lint:eslint": "eslint --ext .js,.ts,.svelte . --ignore-path .gitignore",
    "lint:prettier": "prettier . --check --ignore-path .gitignore --loglevel warn",
    "prepare": "husky install",
    "preview": "svelte-kit preview",
    "ssg": "SSG=1 svelte-kit build"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-graphql-request": "^4.3.3",
    "@graphql-codegen/typescript-operations": "^2.2.3",
    "@popperjs/core": "^2.11.2",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "next",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/adapter-vercel": "next",
    "@sveltejs/kit": "next",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.36",
    "@tsconfig/svelte": "^3.0.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^17.0.14",
    "@types/prosemirror-keymap": "1.0.4",
    "@types/prosemirror-model": "^1.16.0",
    "@types/prosemirror-state": "^1.2.8",
    "@types/prosemirror-view": "^1.23.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "bootstrap": "^5.1.3",
    "cookie": "^0.4.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "graphql-tag": "^2.12.6",
    "husky": "^7.0.4",
    "markdown-it": "^12.3.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-implicit-figures": "^0.10.0",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-replace-link": "^1.1.0",
    "mdsvex": "^0.10.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-plugin-svelte": "^2.6.0",
    "prosemirror-commands": "^1.2.1",
    "prosemirror-dropcursor": "^1.4.0",
    "prosemirror-gapcursor": "^1.2.1",
    "prosemirror-history": "^1.2.0",
    "prosemirror-inputrules": "^1.1.3",
    "prosemirror-keymap": "^1.1.5",
    "prosemirror-markdown": "^1.7.0",
    "prosemirror-menu": "^1.1.4",
    "prosemirror-model": "^1.16.1",
    "prosemirror-schema-basic": "^1.1.2",
    "prosemirror-schema-list": "^1.1.6",
    "prosemirror-state": "^1.3.4",
    "prosemirror-view": "^1.23.6",
    "sass": "^1.49.7",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "svelte": "^3.46.3",
    "svelte-check": "^2.4.1",
    "svelte-preprocess": "^4.10.2",
    "svelte-select": "^4.4.7",
    "svelte-seo": "^1.4.0",
    "swiper": "^8.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "walk": "^2.3.15",
    "y-indexeddb": "^9.0.7",
    "y-prosemirror": "^1.0.14",
    "y-protocols": "^1.0.5",
    "y-webrtc": "^10.2.2",
    "yjs": "^13.5.24"
  },
  "type": "module",
  "dependencies": {
    "svelte-portal": "^2.1.2"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": "eslint --cache --fix",
    "*.{js,ts,svelte,md,json}": "prettier --write"
  }
}
