{
  "name": "svelte-boilerplate",
  "version": "0.0.1",
  "devDependencies": {
    "@tsconfig/svelte": "^1.0.10",
    "@types/marked": "^2.0.2",
    "@types/node": "^15.0.2",
    "@types/spark-md5": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "carbon-icons-svelte": "^10.28.0",
    "cross-env": "7.0.3",
    "derver": "^0.4.15",
    "esbuild": "^0.11.19",
    "esbuild-svelte": "^0.5.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "eslint-svelte3-preprocess": "^0.0.5",
    "husky": "^6.0.0",
    "i18next": "^20.2.2",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-plugin-svelte": "^2.2.0",
    "sass": "^1.32.12",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "svelte": "^3.38.2",
    "svelte-check": "^1.5.2",
    "svelte-pathfinder": "^3.1.3",
    "svelte-preprocess": "^4.7.3",
    "svelte-preprocess-esbuild": "^2.0.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "format": "prettier --plugin=./node_modules/prettier-plugin-svelte --write src",
    "validate": "tsc --noEmit && svelte-check",
    "fix": "eslint --fix src/**/*.{ts,svelte}",
    "lint": "eslint src/**/*.{ts,svelte}",
    "lint-staged": "lint-staged",
    "prepare": "npx husky install",
    "stylelint-check": "stylelint-config-prettier-check",
    "dev": "cross-env NODE_ENV=\"development\" node esbuild.config.js",
    "build": "cross-env NODE_ENV=\"production\" node esbuild.config.js"
  }
}
