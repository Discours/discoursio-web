  "scripts": {
    "gen": "node precompiler.js",
    "codegen": "graphql-codegen",
    "format": "prettier --plugin=./node_modules/prettier-plugin-svelte --write src",
    "validate": "tsc --noEmit && svelte-check",
    "fix:eslint": "eslint --fix src/**/*.{ts,svelte}",
    "fix:style": "stylelint src --fix",
    "fix": "run-p fix:eslint fix:style format",
    "lint:es": "eslint src/**/*.{ts,svelte}",
    "lint-staged": "lint-staged",
    "stylelint-check": "stylelint-config-prettier-check",
    "check": "run-p lint:es validate",
    "copy-assets": "cp -R src/assets/. public/.",
    "build:dev": "cross-env NODE_ENV=\"development\" node esbuild.config.js",
    "dev": "run-s copy-assets build:dev",
    "prod": "cross-env NODE_ENV=\"production\" node esbuild.config.js",
    "build": "run-s gen copy-assets prod ssr",
    "sb:install": "yarn add --save-dev storybook @storybook/svelte @storybook/addon-links @storybook/addon-essentials storybook-addon-turbo-build @storybook/addon-svelte-csf svelte-loader",
    "sb:clean": "rm storybook-static/*",
    "sb:start": "start-storybook -p 6006 -s public",
    "sb:build": "rm storybook-static/* && build-storybook -s public",
    "ssr": "node ssr.js",
    "test": "run-s lint-staged validate"
  },
  "resolutions": {
    "esbuild": "0.12.19",
    "graphql": "15.5.0"
  },
  "type": "module",
  "dependencies": {
    "marked": "^3.0.0"
  }
}
