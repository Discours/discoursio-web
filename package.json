{
  "name": "discoursio-web",
  "version": "0.0.5",
  "private": "true",
  "license": "MIT",
  "description": "Collaborative publishing",
  "repository": {
    "url": "https://github.com/Discours/discoursio-web"
  },
  "scripts": {
    "dev": "run-s gen dev:sk",
    "dev:sk": "svelte-kit dev",
    "build:sk": "svelte-kit build",
    "build": "run-s gen build:sk",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "fix": "eslint --fix --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "gen": "node precompiler.js",
    "codegen": "graphql-codegen",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "latest",
    "@graphql-codegen/typescript": "latest",
    "@graphql-codegen/typescript-operations": "latest",
