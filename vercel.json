{
    "headers": [
      {
        "source": "/auth/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/((?!api/.*).*)",
        "destination": "/index.html"
      }
    ]
  }