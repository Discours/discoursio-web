{    
    "headers": [
        {
            "source": "/auth/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache"
                }
            ]
        }
    ],
    "rewrites": [
        { "source": "/api/:match*", "destination": "https://newapi.discours.io/:match*" }
    ]
}